<LinearLayout
    android:padding="5dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    android:orientation="vertical"
    android:weightSum="7"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:id="@+id/TimePeriodLinearLayout"
        android:padding="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"


        android:layout_weight="1"
        android:weightSum="90"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="30"


            android:text="Time Period"></TextView>

        <TextView
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="10"



            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=":"></TextView>


        <TextView
            android:id="@+id/TimePeriod"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="50"


            android:text="Time Period"
            ></TextView>



    </LinearLayout>


    <LinearLayout
        android:id="@+id/ClientLinearLayout"
        android:padding="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"


        android:layout_weight="1"
        android:weightSum="90"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="30"



            android:text="Client"></TextView>

        <TextView
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="10"



            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=":"></TextView>


        <TextView
            android:id="@+id/Client"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="50"



            android:text="Client"
            ></TextView>



    </LinearLayout>



    <LinearLayout
        android:id="@+id/PrimaryVendorLinearLayout"
        android:padding="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"


        android:layout_weight="1"
        android:weightSum="90"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="30"



            android:text="Primary Vendor"></TextView>

        <TextView
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="10"



            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=":"></TextView>


        <TextView
            android:id="@+id/PrimaryVendor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="50"



            android:text="Primary Vendor"
            ></TextView>



    </LinearLayout>
    <LinearLayout
        android:id="@+id/SecondaryVendorLinearLayout"
        android:padding="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"


        android:layout_weight="1"
        android:weightSum="90"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="30"



            android:text="Secondary Vendor"></TextView>

        <TextView
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="10"



            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=":"></TextView>


        <TextView
            android:id="@+id/SecondaryVendor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="50"



            android:text="Secondary Vendor"></TextView>



    </LinearLayout>

    <LinearLayout
        android:id="@+id/ThirdVendorLinearLayout"
        android:padding="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="90"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="30"
            android:text="Third Vendor"></TextView>

        <TextView
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=":"></TextView>


        <TextView
            android:id="@+id/ThirdVendor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="50"
            android:text="Third Vendor"></TextView>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/HourlyRateLinearLayout"
        android:padding="5dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="90"
        android:layout_weight="1"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="30"
            android:text="Hourly Rate"></TextView>

        <TextView
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_weight="10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=":"></TextView>


        <TextView
            android:id="@+id/HourlyRate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="50"
            android:text="Hourly Rate"></TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="right|bottom"
        android:gravity="right"
        android:layout_weight="1"
        android:layout_margin="20dp">

        <TextView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/edit"
            android:id="@+id/Edit"
            >

        </TextView>
        <TextView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/delete"
            android:id="@+id/Delete"
            >

        </TextView>
    </LinearLayout>

</LinearLayout>